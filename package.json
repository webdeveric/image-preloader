{
  "name": "image-preloader",
  "version": "0.4.0",
  "description": "Preload images using promises",
  "private": true,
  "author": {
    "name": "Eric King",
    "email": "eric@webdeveric.com",
    "url": "http://webdeveric.com/"
  },
